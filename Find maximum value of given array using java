public class Main
{
	  public static int largest(int arr[]){
      int largest = Integer.MIN_VALUE;
      int smallest = Integer.MAX_VALUE;
      
        for(int i = 0;i<arr.length;i++){
            if(largest<arr[i]){
                largest=arr[i];
               
            }
            if(smallest>arr[i]){
                smallest =arr[i];
            }
        }
         System.out.println("The higest element of array " + largest);
         System.out.println("The smallest element of array " + smallest);
        
        return largest;
    }
    public static void main(String[] args) {
        int arr[] = {5,6,7,8,9,3,12,34,6,4};
        largest(arr);
    }
}
